Object subclass: #Locomotora	instanceVariableNames: 'velocidadMaxima peso kilosArrastre'	classVariableNames: ''	poolDictionaries: ''	category: 'EjercicioTrenes'!!Locomotora methodsFor: 'as yet unclassified' stamp: 'GermanLeiva 10/5/2010 11:36'!arrastreUtil	^kilosArrastre - peso! !!Locomotora methodsFor: 'as yet unclassified' stamp: 'GermanLeiva 10/5/2010 11:32'!esEficiente	^kilosArrastre >= (peso * 5)! !!Locomotora methodsFor: 'as yet unclassified' stamp: 'GermanLeiva 10/5/2010 11:33'!kilosArrastre: aSmallInteger 	kilosArrastre := aSmallInteger ! !!Locomotora methodsFor: 'as yet unclassified' stamp: 'GermanLeiva 10/5/2010 11:34'!peso: aSmallInteger 	peso := aSmallInteger ! !!Locomotora methodsFor: 'as yet unclassified' stamp: 'GermanLeiva 10/5/2010 11:30'!velocidadMaxima	^velocidadMaxima ! !!Locomotora methodsFor: 'as yet unclassified' stamp: 'GermanLeiva 10/5/2010 11:31'!velocidadMaxima: cantidad	velocidadMaxima := cantidad ! !Object subclass: #Tren	instanceVariableNames: 'vagones locomotoras'	classVariableNames: ''	poolDictionaries: ''	category: 'EjercicioTrenes'!!Tren methodsFor: 'as yet unclassified' stamp: 'GermanLeiva 10/5/2010 11:30'!agregarLocomotora: unaLocomotora 	locomotoras add: unaLocomotora ! !!Tren methodsFor: 'as yet unclassified' stamp: 'GermanLeiva 10/5/2010 11:21'!agregarVagon: unVagon	vagones add: unVagon! !!Tren methodsFor: 'as yet unclassified' stamp: 'GermanLeiva 10/5/2010 11:35'!arrastreLocomotoras	^locomotoras inject: 0 into: [ :arrastreTotal :unaLoco | unaLoco arrastreUtil + arrastreTotal ]! !!Tren methodsFor: 'as yet unclassified' stamp: 'GermanLeiva 10/5/2010 11:25'!cantidadVagonesLivianos		^(vagones select: [:unVagon | unVagon esLiviano]) size! !!Tren methodsFor: 'as yet unclassified' stamp: 'GermanLeiva 10/5/2010 11:19'!initialize	vagones := Set new.	locomotoras := Set new.! !!Tren methodsFor: 'as yet unclassified' stamp: 'GermanLeiva 10/5/2010 11:38'!kilosEmpujeFaltante	self puedeMoverse ifTrue: [ ^0 ].	^self pesoVagones - self arrastreLocomotoras ! !!Tren methodsFor: 'as yet unclassified' stamp: 'GermanLeiva 10/5/2010 11:36'!pesoVagones	^vagones inject: 0 into: [ :pesoTotal :unVagon | unVagon pesoMaximo + pesoTotal ]! !!Tren methodsFor: 'as yet unclassified' stamp: 'GermanLeiva 10/5/2010 11:35'!puedeMoverse	^self arrastreLocomotoras >= self pesoVagones! !!Tren methodsFor: 'as yet unclassified' stamp: 'GermanLeiva 10/5/2010 11:32'!sosEficiente	^locomotoras allSatisfy: [ :unaLoco | unaLoco esEficiente ]! !!Tren methodsFor: 'as yet unclassified' stamp: 'GermanLeiva 10/5/2010 11:18'!totalPasajeros	^vagones 			inject: 0 			into: [ :total :unVagon | unVagon capacidad + total ]! !!Tren methodsFor: 'as yet unclassified' stamp: 'GermanLeiva 10/5/2010 11:30'!velocidadMaxima	^locomotoras			inject: locomotoras anyOne velocidadMaxima			into: [ :velocidadMinima :unaLoco |					unaLoco velocidadMaxima min: velocidadMinima ]! !Object subclass: #VagonCarga	instanceVariableNames: 'cargaMaxima pesoMaximo'	classVariableNames: ''	poolDictionaries: ''	category: 'EjercicioTrenes'!!VagonCarga methodsFor: 'as yet unclassified' stamp: 'GermanLeiva 10/5/2010 11:21'!capacidad	^0! !!VagonCarga methodsFor: 'as yet unclassified' stamp: 'GermanLeiva 10/5/2010 11:25'!esLiviano	^self pesoMaximo < 2500! !!VagonCarga methodsFor: 'as yet unclassified' stamp: 'GermanLeiva 10/5/2010 11:27'!pesoMaximo	^pesoMaximo! !!VagonCarga methodsFor: 'accessing' stamp: 'GermanLeiva 10/5/2010 11:26'!cargaMaxima: anObject	cargaMaxima := anObject! !!VagonCarga methodsFor: 'accessing' stamp: 'GermanLeiva 10/5/2010 11:27'!pesoMaximo: anObject	pesoMaximo := anObject! !Object subclass: #VagonPasajeros	instanceVariableNames: 'largo anchoUtil'	classVariableNames: ''	poolDictionaries: ''	category: 'EjercicioTrenes'!!VagonPasajeros methodsFor: 'as yet unclassified' stamp: 'GermanLeiva 10/5/2010 11:22'!capacidad	anchoUtil <= 2.5		ifTrue: [^largo * 8].	^largo * 10! !!VagonPasajeros methodsFor: 'as yet unclassified' stamp: 'GermanLeiva 10/5/2010 11:28'!esLiviano	^self pesoMaximo < 2500! !!VagonPasajeros methodsFor: 'as yet unclassified' stamp: 'GermanLeiva 10/5/2010 11:28'!pesoMaximo	^self capacidad * 80! !!VagonPasajeros methodsFor: 'accessing' stamp: 'GermanLeiva 10/5/2010 11:23'!anchoUtil: anObject	anchoUtil := anObject! !!VagonPasajeros methodsFor: 'accessing' stamp: 'GermanLeiva 10/5/2010 11:23'!largo: anObject	largo := anObject! !