Object subclass: #Lobo	instanceVariableNames: 'calorias'	classVariableNames: ''	poolDictionaries: ''	category: 'LosTresChanchitos'!!Lobo commentStamp: '<historical>' prior: 0!!!Lobo methodsFor: 'accessing' stamp: 'JessSaavedra 9/2/2015 15:38'!calorias	^ calorias! !!Lobo methodsFor: 'accessing' stamp: 'JessSaavedra 9/2/2015 15:38'!calorias: anObject	calorias := anObject! !!Lobo methodsFor: 'as yet unclassified' stamp: 'JessSaavedra 9/2/2015 16:25'!perderCalorias: cantidadcalorias:= 0 max: (calorias - cantidad)! !!Lobo methodsFor: 'as yet unclassified' stamp: 'JessSaavedra 9/2/2015 16:39'!comer: alguienself calorias: calorias + (alguien caloriasQueAporta)! !!Lobo methodsFor: 'as yet unclassified' stamp: 'JessSaavedra 9/2/2015 15:43'!estaSaludable^calorias between: 20 and: 150! !!Lobo methodsFor: 'as yet unclassified' stamp: 'JessSaavedra 9/2/2015 15:38'!estaGordo^calorias > 200! !!Lobo methodsFor: 'as yet unclassified' stamp: 'JessSaavedra 9/2/2015 16:39'!correr: minutosself perderCalorias: (2*minutos)! !!Lobo methodsFor: 'as yet unclassified' stamp: 'JessSaavedra 9/2/2015 16:18'!soplar: algoself perderCalorias: (algo resistenciaTotal)! !TestCase subclass: #TestsTresChanchitos	instanceVariableNames: 'porky babe yamilo piglet superChancho casaDePaja casaDeMadera caperucita'	classVariableNames: ''	poolDictionaries: ''	category: 'LosTresChanchitos'!!TestsTresChanchitos commentStamp: '<historical>' prior: 0!!!TestsTresChanchitos methodsFor: 'as yet unclassified' stamp: 'JessSaavedra 9/2/2015 16:23'!testYamiloSoplaCasaDeMaderaConPigletcasaDeMadera llega: piglet.yamilo soplar: casaDeMadera.self assert: (yamilo calorias) equals: 83.! !!TestsTresChanchitos methodsFor: 'as yet unclassified' stamp: 'JessSaavedra 9/2/2015 16:35'!setUpporky := Chancho new.porky peso: 30.piglet := Chancho new.piglet peso: 12.superChancho := Chancho new.superChancho peso: 2000.yamilo:= Lobo new.yamilo calorias: 100.casaDePaja := Casa new.casaDePaja resistencia: 0.casaDeMadera := Casa new.casaDeMadera resistencia: 5.caperucita := Caperucita new.caperucita calorias: 55.caperucita cantidadDeManzanas: 5.! !!TestsTresChanchitos methodsFor: 'as yet unclassified' stamp: 'JessSaavedra 9/2/2015 16:24'!testYamiloSoplaCasaDePajaConSuperChanchocasaDePaja llega: superChancho.yamilo soplar: casaDePaja.self assert: (yamilo calorias) equals: 0.! !!TestsTresChanchitos methodsFor: 'as yet unclassified' stamp: 'JessSaavedra 9/2/2015 16:35'!testYamiloComeACaperucitayamilo comer: caperucita.self assert: (yamilo calorias) equals: 165.! !!TestsTresChanchitos methodsFor: 'as yet unclassified' stamp: 'JessSaavedra 9/2/2015 16:04'!testYamiloCorre10minutosyamilo correr: 10.self assert: (yamilo calorias) equals: 80! !!TestsTresChanchitos methodsFor: 'as yet unclassified' stamp: 'JessSaavedra 9/2/2015 16:28'!testYamiloSoplaCasaDePajaConPorkyYPigletcasaDePaja llega: porky.casaDePaja llega: piglet.yamilo soplar: casaDePaja.self assert: (yamilo calorias) equals: 58.! !!TestsTresChanchitos methodsFor: 'as yet unclassified' stamp: 'JessSaavedra 9/2/2015 16:27'!testYamiloComePorkyyamilo comer: porky.self assert: (yamilo calorias) equals: 103.! !!TestsTresChanchitos methodsFor: 'as yet unclassified' stamp: 'JessSaavedra 9/2/2015 15:53'!testYamiloNoEstaGordoself deny: (yamilo estaGordo)! !!TestsTresChanchitos methodsFor: 'as yet unclassified' stamp: 'JessSaavedra 9/2/2015 15:53'!testYamiloEstaSaludableself assert: (yamilo estaSaludable)! !!TestsTresChanchitos methodsFor: 'as yet unclassified' stamp: 'JessSaavedra 9/2/2015 16:28'!testYamiloEstaGordoDespuesDeComerSuperChanchoyamilo comer: superChancho.self assert: (yamilo estaGordo)! !Object subclass: #Casa	instanceVariableNames: 'ocupantes resistencia'	classVariableNames: ''	poolDictionaries: ''	category: 'LosTresChanchitos'!!Casa commentStamp: '<historical>' prior: 0!!!Casa methodsFor: 'as yet unclassified' stamp: 'JessSaavedra 9/2/2015 16:19'!resistenciaTotal^self resistencia + self pesoOcupantes! !!Casa methodsFor: 'as yet unclassified' stamp: 'JessSaavedra 9/2/2015 16:09'!llega: alguienocupantes add: alguien! !!Casa methodsFor: 'as yet unclassified' stamp: 'JessSaavedra 9/2/2015 16:13'!initializeocupantes := Bag new.! !!Casa methodsFor: 'accessing' stamp: 'JessSaavedra 9/2/2015 16:13'!ocupantes	^ ocupantes! !!Casa methodsFor: 'accessing' stamp: 'JessSaavedra 9/2/2015 16:13'!resistencia	^ resistencia! !!Casa methodsFor: 'accessing' stamp: 'JessSaavedra 9/2/2015 16:19'!pesoOcupantes^ocupantes sum: [ :ocupante | ocupante peso ]! !!Casa methodsFor: 'accessing' stamp: 'JessSaavedra 9/2/2015 16:13'!resistencia: anObject	resistencia := anObject! !Object subclass: #Caperucita	instanceVariableNames: 'calorias manzanas'	classVariableNames: ''	poolDictionaries: ''	category: 'LosTresChanchitos'!!Caperucita commentStamp: '<historical>' prior: 0!!!Caperucita methodsFor: 'accessing' stamp: 'JessSaavedra 9/2/2015 16:30'!calorias	^ calorias! !!Caperucita methodsFor: 'accessing' stamp: 'JessSaavedra 9/2/2015 16:30'!calorias: anObject	calorias := anObject! !!Caperucita methodsFor: 'accessing' stamp: 'JessSaavedra 9/2/2015 16:32'!cantidadDeManzanas	^ manzanas! !!Caperucita methodsFor: 'accessing' stamp: 'JessSaavedra 9/2/2015 16:32'!cantidadDeManzanas: anObject	manzanas := anObject! !!Caperucita methodsFor: 'as yet unclassified' stamp: 'JessSaavedra 9/2/2015 16:32'!caloriasQueAporta^self calorias + (2* self cantidadDeManzanas)! !Object subclass: #Chancho	instanceVariableNames: 'peso'	classVariableNames: ''	poolDictionaries: ''	category: 'LosTresChanchitos'!!Chancho commentStamp: '<historical>' prior: 0!!!Chancho methodsFor: 'as yet unclassified' stamp: 'JessSaavedra 9/2/2015 15:42'!caloriasQueAporta^self peso / 10! !!Chancho methodsFor: 'accessing' stamp: 'JessSaavedra 9/2/2015 15:41'!peso	^ peso! !!Chancho methodsFor: 'accessing' stamp: 'JessSaavedra 9/2/2015 15:41'!peso: anObject	peso := anObject! !