Error subclass: #NoHayMonstruosParaAtacarException	instanceVariableNames: ''	classVariableNames: ''	poolDictionaries: ''	category: 'PdeP-VanHelsing'!Object subclass: #Monstruo	instanceVariableNames: 'vitalidad ataquesEnLosQueParticipo'	classVariableNames: ''	poolDictionaries: ''	category: 'PdeP-VanHelsing'!!Monstruo methodsFor: 'as yet unclassified' stamp: 'MarianaMatos 10/8/2012 22:32'!esRapido 	^ self velocidad > 100! !!Monstruo methodsFor: 'as yet unclassified' stamp: 'MarianaMatos 10/8/2012 23:46'!initialize  	ataquesEnLosQueParticipo := OrderedCollection new! !!Monstruo methodsFor: 'as yet unclassified' stamp: 'MarianaMatos 10/8/2012 23:52'!participarEnAtaque:  unAtaque	ataquesEnLosQueParticipo add: unAtaque.	unAtaque monstruos add: self.! !!Monstruo methodsFor: 'as yet unclassified' stamp: 'MarianaMatos 10/9/2012 00:02'!patetico 	^ ataquesEnLosQueParticipo anySatisfy: [:ataque | ataque zonaPreviamenteDestruida]! !!Monstruo methodsFor: 'as yet unclassified' stamp: 'MarianaMatos 10/8/2012 14:37'!peligrosidad 	^ self vitalidad * self velocidad! !!Monstruo methodsFor: 'accessing' stamp: 'MarianaMatos 10/8/2012 14:38'!vitalidad	^ vitalidad! !!Monstruo methodsFor: 'accessing' stamp: 'MarianaMatos 10/8/2012 14:38'!vitalidad: anObject	vitalidad := anObject! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!Monstruo class	instanceVariableNames: ''!!Monstruo class methodsFor: 'as yet unclassified' stamp: 'MarianaMatos 10/8/2012 15:04'!conVitalidad: unaVitalidad 	^ self new		vitalidad: unaVitalidad ;		yourself ! !Object subclass: #Zona	instanceVariableNames: 'ataques'	classVariableNames: ''	poolDictionaries: ''	category: 'PdeP-VanHelsing'!!Zona methodsFor: 'accessing' stamp: 'MarianaMatos 10/9/2012 00:14'!agregarAtaque: monstruos	|ataque|	ataque := Ataque zona: self.	monstruos do: [:monstruo | monstruo participarEnAtaque: ataque ].	self ataques add: ataque.! !!Zona methodsFor: 'accessing' stamp: 'MarianaMatos 10/8/2012 15:11'!ataques	^ ataques! !!Zona methodsFor: 'accessing' stamp: 'MarianaMatos 10/8/2012 15:11'!ataques: anObject	ataques := anObject! !!Zona methodsFor: 'accessing' stamp: 'MarianaMatos 10/8/2012 23:58'!fueDestruida	^ self ataques anySatisfy: [:ataque | ataque destructivo]! !!Zona methodsFor: 'accessing' stamp: 'MarianaMatos 10/8/2012 22:29'!initialize 	ataques := OrderedCollection new! !!Zona methodsFor: 'accessing' stamp: 'MarianaMatos 10/8/2012 22:31'!monstruosRapidos 	^ ataques inject: Set new into: [:rapidos :ataque | 		rapidos union: ataque monstruosRapidos ]! !!Zona methodsFor: 'accessing' stamp: 'MarianaMatos 10/8/2012 15:12'!nightmareTeam	^ (ataques collect: [:ataque | ataque monstruoMasPeligroso ]) asSet! !!Zona methodsFor: 'accessing' stamp: 'MarianaMatos 10/9/2012 00:18'!registrarAtaque: monstruos	|puedenAtacar|	puedenAtacar := self filtrarMonstruosQuePuedenAtacarDe: monstruos.	puedenAtacar ifEmpty: [NoHayMonstruosParaAtacarException signal: 'Ninguno de los monstruos elegidos puede atacar a la zona'].	self agregarAtaque: puedenAtacar ! !Zona subclass: #Aldea	instanceVariableNames: ''	classVariableNames: ''	poolDictionaries: ''	category: 'PdeP-VanHelsing'!!Aldea methodsFor: 'as yet unclassified' stamp: 'MarianaMatos 10/9/2012 00:19'!filtrarMonstruosQuePuedenAtacarDe: monstruos 	^ monstruos reject: [:monstruo | monstruo patetico]! !!Aldea methodsFor: 'as yet unclassified' stamp: 'MarianaMatos 10/8/2012 23:25'!resistencia	^ 2000! !Object subclass: #Ataque	instanceVariableNames: 'monstruos fecha zona'	classVariableNames: ''	poolDictionaries: ''	category: 'PdeP-VanHelsing'!!Ataque methodsFor: 'as yet unclassified' stamp: 'MarianaMatos 10/8/2012 23:58'!destructivo 	^self nivelDeDevastacion > self zona resistencia! !!Ataque methodsFor: 'as yet unclassified' stamp: 'MarianaMatos 10/8/2012 15:07'!monstruoMasPeligroso	^ (monstruos asSortedCollection: [:m1 :m2 | m1 peligrosidad > m2 peligrosidad ]) first! !!Ataque methodsFor: 'as yet unclassified' stamp: 'MarianaMatos 10/8/2012 22:31'!monstruosRapidos	^monstruos select: [:monstruo| monstruo esRapido ]! !!Ataque methodsFor: 'as yet unclassified' stamp: 'MarianaMatos 10/8/2012 23:10'!nivelDeDevastacion	^(monstruos collect:  [:monstruo| monstruo peligrosidad  ]) sum! !!Ataque methodsFor: 'accessing' stamp: 'MarianaMatos 10/9/2012 00:03'!fecha	^ fecha! !!Ataque methodsFor: 'accessing' stamp: 'MarianaMatos 10/9/2012 00:03'!fecha: anObject	fecha := anObject! !!Ataque methodsFor: 'accessing' stamp: 'MarianaMatos 10/9/2012 00:03'!initialize 	monstruos := Set new.	fecha := Date today.! !!Ataque methodsFor: 'accessing' stamp: 'MarianaMatos 10/8/2012 14:58'!monstruos	^ monstruos! !!Ataque methodsFor: 'accessing' stamp: 'MarianaMatos 10/8/2012 14:58'!monstruos: anObject	monstruos := anObject! !!Ataque methodsFor: 'accessing' stamp: 'MarianaMatos 10/8/2012 23:47'!zona	^ zona! !!Ataque methodsFor: 'accessing' stamp: 'MarianaMatos 10/8/2012 23:47'!zona: anObject	zona := anObject! !!Ataque methodsFor: 'accessing' stamp: 'MarianaMatos 10/9/2012 00:03'!zonaPreviamenteDestruida	^self  zona ataques		anySatisfy: [:ataque | ataque fecha < self fecha								and: [ataque destructivo ]]! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!Ataque class	instanceVariableNames: ''!!Ataque class methodsFor: 'as yet unclassified' stamp: 'MarianaMatos 10/8/2012 23:47'!zona: unaZona	^ self new zona: unaZona ! !Zona subclass: #Castillo	instanceVariableNames: 'plusResistencia'	classVariableNames: ''	poolDictionaries: ''	category: 'PdeP-VanHelsing'!!Castillo methodsFor: 'as yet unclassified' stamp: 'MarianaMatos 10/9/2012 00:20'!filtrarMonstruosQuePuedenAtacarDe: monstruos 	^ monstruos select: [:monstruo | monstruo peligrosidad > 256]! !!Castillo methodsFor: 'as yet unclassified' stamp: 'MarianaMatos 10/8/2012 23:28'!plusResistencia: unPlus 	plusResistencia := unPlus ! !!Castillo methodsFor: 'as yet unclassified' stamp: 'MarianaMatos 10/8/2012 23:26'!resistencia	^ 3000 + plusResistencia ! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!Castillo class	instanceVariableNames: ''!!Castillo class methodsFor: 'as yet unclassified' stamp: 'MarianaMatos 10/8/2012 23:27'!conPlus: unPlus^ 	self new plusResistencia: unPlus ! !Monstruo subclass: #Dragon	instanceVariableNames: 'velocidad'	classVariableNames: ''	poolDictionaries: ''	category: 'PdeP-VanHelsing'!!Dragon methodsFor: 'accessing' stamp: 'MarianaMatos 10/8/2012 14:41'!velocidad	^ velocidad! !!Dragon methodsFor: 'accessing' stamp: 'MarianaMatos 10/8/2012 14:41'!velocidad: anObject	velocidad := anObject! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!Dragon class	instanceVariableNames: ''!!Dragon class methodsFor: 'as yet unclassified' stamp: 'MarianaMatos 10/8/2012 15:04'!conVitalidad: unaVitalidad  velocidad: unaVelocidad	^(self conVitalidad: unaVitalidad)			velocidad: unaVelocidad ;			yourself! !Monstruo subclass: #HombreLobo	instanceVariableNames: ''	classVariableNames: ''	poolDictionaries: ''	category: 'PdeP-VanHelsing'!!HombreLobo methodsFor: 'as yet unclassified' stamp: 'MarianaMatos 10/8/2012 14:39'!velocidad 	^ self vitalidad * 2 + 30! !Monstruo subclass: #SapoPepe	instanceVariableNames: ''	classVariableNames: ''	poolDictionaries: ''	category: 'PdeP-VanHelsing'!!SapoPepe methodsFor: 'as yet unclassified' stamp: 'MarianaMatos 10/9/2012 00:28'!patetico 	^ true! !!SapoPepe methodsFor: 'as yet unclassified' stamp: 'MarianaMatos 10/9/2012 00:27'!peligrosidad 	^  2000000! !!SapoPepe methodsFor: 'as yet unclassified' stamp: 'MarianaMatos 10/9/2012 00:26'!velocidad	^ 150000! !Monstruo subclass: #Vampiro	instanceVariableNames: ''	classVariableNames: 'Velocidad'	poolDictionaries: ''	category: 'PdeP-VanHelsing'!!Vampiro methodsFor: 'as yet unclassified' stamp: 'MarianaMatos 10/8/2012 14:41'!velocidad	^ self class velocidad ! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!Vampiro class	instanceVariableNames: ''!!Vampiro class methodsFor: 'as yet unclassified' stamp: 'MarianaMatos 10/8/2012 14:40'!velocidad 	^ Velocidad ! !!Vampiro class methodsFor: 'as yet unclassified' stamp: 'MarianaMatos 10/8/2012 14:40'!velocidad: unaVelocidad 	Velocidad := unaVelocidad ! !